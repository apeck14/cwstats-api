{
  "name": "cwstats-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "build": "tsc && tsc-alias",
    "tunnel": "ssh -L 6379:127.0.0.1:6379 VPS -N",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "lint": "xo-lint",
    "lint:fix": "xo-lint --fix",
    "test": "jest --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "update:minor": "ncu -u --target minor",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "keywords": [],
  "author": "Apehk",
  "license": "ISC",
  "description": "API for CWStats",
  "dependencies": {
    "@logtail/node": "^0.5.6",
    "@logtail/winston": "^0.5.6",
    "@panva/hkdf": "^1.2.1",
    "axios": "^1.13.4",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jose": "^5.10.0",
    "lodash": "^4.17.23",
    "mongoose": "^8.22.0",
    "stripe": "^20.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "winston": "^3.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-validator": "^2.20.33",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.23",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "xo-wrapper": "^3.7.1"
  },
  "overrides": {
    "eslint": "9.38.0"
  }
}
