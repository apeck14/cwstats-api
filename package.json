{
  "name": "cwstats-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "lint": "xo-lint",
    "lint:fix": "xo-lint --fix",
    "test": "jest --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "update:minor": "ncu -u --target minor",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "keywords": [],
  "author": "Apehk",
  "license": "ISC",
  "description": "API for CWStats",
  "dependencies": {
    "@logtail/node": "^0.5.5",
    "@logtail/winston": "^0.5.5",
    "@panva/hkdf": "^1.2.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jose": "^5.10.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.17.2",
    "stripe": "^18.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-validator": "^2.20.33",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "xo-wrapper": "^3.4.15"
  },
  "overrides": {
    "eslint": "9.38.0"
  }
}
